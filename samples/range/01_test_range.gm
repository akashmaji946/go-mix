
// Test 1: Create a range and print it
var x = 2...5;
println(x);

// Test 2: Use foreach with a range literal
foreach i in 1...3 {
    println(i);
}

// Test 3: Use foreach with a range variable
var myRange = 10...12;
foreach num in myRange {
    println(num);
}

// Test 4: Use foreach with an array
foreach item in [100, 200, 300] {
    println(item);
}
