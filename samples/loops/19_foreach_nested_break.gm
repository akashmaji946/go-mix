// Nested foreach loops with break/continue

println("--- Nested Foreach ---");
var matrix = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
];

foreach row in matrix {
    println("Row start");
    foreach val in row {
        if (val == 5) {
            println("  Found 5, skipping");
            continue;
        }
        if (val == 8) {
            println("  Found 8, breaking inner");
            break;
        }
        println("  Val: ", val);
    }
    println("Row end");
}