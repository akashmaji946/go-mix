// Test break and continue in foreach loops

println("--- Foreach Range ---");
foreach x in 1...10 {
    if (x % 3 == 0) {
        continue;
    }
    if (x > 7) {
        break;
    }
    print(x, " ");
}
println("");

println("\n--- Foreach Array ---");
var arr = [10, 20, 30, 40, 50, 60, 70, 80, 90];
foreach item in arr {
    if (item == 30) {
        continue;
    }
    if (item > 40) {
        break;
    }
    println(item);
}