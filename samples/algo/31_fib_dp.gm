// Fibonacci with DP (Bottom-Up)

func fib(n) {
    if (n <= 1) { return n; }
    
    var dp = [];
    // Initialize array
    for (var i = 0; i <= n; i = i + 1) {
        dp = push(dp, 0);
    }
    
    dp[0] = 0;
    dp[1] = 1;
    
    for (var i = 2; i <= n; i = i + 1) {
        dp[i] = dp[i-1] + dp[i-2];
    }
    
    return dp[n];
}

var n = 10;
println("Fibonacci(", n, ") = ", fib(n));